{
  "name": "next-edge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:edge": "npx @cloudflare/next-on-pages --experimental-minify",
    "build:watch": "npx @cloudflare/next-on-pages --watch",
    "dev:pages": "npx wrangler pages dev .vercel/output/static --compatibility-flags=transformstream_enable_standard_constructor --compatibility-flags=streams_enable_constructors",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.27.0",
    "eslint-config-next": "^12.3.0",
    "next": "^12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "github:julesyoungberg/next-on-pages",
    "vercel": "^28.5.4"
  }
}
